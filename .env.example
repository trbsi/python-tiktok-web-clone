# General
APP_NAME=MyApp
APP_ENV=local
APP_URL=https://www.test.com
SECRET_KEY="abc"
DEBUG=True
ALLOWED_HOSTS="127.0.0.1"
DB_TYPE=mysql
COMPOSE_PROJECT_NAME=myproject

# Email
DEFAULT_FROM_EMAIL="support@test.com"
