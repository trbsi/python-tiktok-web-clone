# Django
DJANGO_SETTINGS_MODULE=app.settings
DJANGO_WSGI=app.wsgi
DJANGO_CREATE_SUPERUSER=true
DJANGO_SUPERUSER_USERNAME=admin
DJANGO_SUPERUSER_EMAIL=admin@example.com
DJANGO_SUPERUSER_PASSWORD=admin

# Database
MYSQL_ROOT_PASSWORD=rootpass
MYSQL_DATABASE=django_db
MYSQL_USER=django
MYSQL_PASSWORD=pass
MYSQL_HOST=my-app-db
DATABASE_URL=mysql://django:pass@db:3306/django_db
PHPMYADMIN_VIRTUAL_HOST=mojasuperbaza.example.com #For local dev it is empty
MYSQL_HOST_PORT=3306

# Celery
CELERY_APP=celery #this is name of app/celery.py
CELERY_BROKER_URL=redis://my-app-redis:6379/0
CELERY_RESULT_BACKEND=redis://my-app-redis:6379/0
CELERY_TASK_ALWAYS_EAGER=False # For local dev is True, to run sync
CELERY_TASK_EAGER_PROPAGATES=True # raises exceptions instead of swallowing them

# Gunicorn
WEB_CONCURRENCY=3
GUNICORN_TIMEOUT=30
GUNICORN_LOG_LEVEL=info

